#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/sites/b2bshop/node_modules/.pnpm/node-sass@7.0.3/node_modules/node-sass/bin/node_modules:/sites/b2bshop/node_modules/.pnpm/node-sass@7.0.3/node_modules/node-sass/node_modules:/sites/b2bshop/node_modules/.pnpm/node-sass@7.0.3/node_modules:/sites/b2bshop/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/sites/b2bshop/node_modules/.pnpm/node-sass@7.0.3/node_modules/node-sass/bin/node_modules:/sites/b2bshop/node_modules/.pnpm/node-sass@7.0.3/node_modules/node-sass/node_modules:/sites/b2bshop/node_modules/.pnpm/node-sass@7.0.3/node_modules:/sites/b2bshop/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../node-sass@7.0.3/node_modules/node-sass/bin/node-sass" "$@"
else
  exec node  "$basedir/../../../../../node-sass@7.0.3/node_modules/node-sass/bin/node-sass" "$@"
fi
