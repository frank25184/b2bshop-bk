<nav class="pagination is-rounded" role="navigation" aria-label="pagination" style="margin-top:1rem;">
 
  {{#unless isFirstPage }}
    {{#if query}}
      <a class="pagination-previous" href="?p={{ math page "-" "1" }}{{queryString}}">Previous</a>
    {{else}}
     <a class="pagination-previous" href="?p={{ math page "-" "1" }}">Previous</a>
    {{/if}}
  {{/unless}}


  {{#unless isLastPage }}
      {{#if query}}
      <a class="pagination-next" href="?p={{ math page "+" "1" }}{{queryString}}">Next</a>
      {{else}}
      <a class="pagination-next" href="?p={{ math page "+" "1" }}">Next page</a>
      {{/if}}
  {{/unless}}


</nav>









