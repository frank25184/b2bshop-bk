  
{{#extend "css"}}
<link rel="canonical" href="">
{{/extend}}

      <!-- banner -->
      <section class="sb-banner sb-banner-xs sb-banner-color supportTop">
        <div class="container">
          <div class="row">
            <div class="col-lg-12">
              <!-- main title -->
              <div class="sb-main-title-frame">
                <div class="sb-main-title">
                  <h1 class="sb-h2">Our Blog</h1>
                  <ul class="sb-breadcrumbs">
                    <li><a href="/">Home</a></li>
                    <li><a href="/articles">Blog</a></li>
                  </ul>
                </div>
              </div>
              <!-- main title end -->
            </div>
          </div>
        </div>
      </section>
      <!-- banner end -->

      <!-- blog list -->
      <section class="sb-blog-list sb-p-90-90">
        <div class="sb-bg-1">
          <div></div>
        </div>
        <div class="container">
          <div class="sb-mb-60">
            <h2 class="sb-cate-title sb-mb-15">Latest <span>Articles</span></h2>
            <p class="sb-text">Latest articles about physiotherapy equipment.</p>
          </div>
          <div class="sb-masonry-grid">
            <div class="sb-grid-sizer"></div>
            {{#if articles.length}}
            {{#each articles}}
            <div class="sb-grid-item sb-item-33">
              <a href="/article/{{this.pathName}}" class="sb-blog-card sb-mb-30">
                <div class="sb-cover-frame sb-cover-vert sb-mb-30">
                  <img src="/upload/articles/thumbnail/{{this.topIMG}}" alt="cover"> 
                </div>
                <div class="sb-blog-card-descr">
                  <h3 class="sb-mb-10">{{this.title}}</h3>
                  <div class="sb-suptitle sb-mb-15"><span>{{this.updated_at}}</span></div>
                  <p class="sb-text">{{this.intro}}</p>
                </div>
              </a>
            </div>
            {{/each}}
            {{else}}
            <div class="sb-grid-item sb-item-33">
              <a href="#" class="sb-blog-card sb-mb-30">
                <div class="sb-cover-frame sb-cover-vert sb-mb-30">
                  <img src="/images/picture/factory.jpg" alt="cover"> 
                </div>
                <div class="sb-blog-card-descr">
                  <h3 class="sb-mb-10">No articles found</h3>
                </div>
              </a>
            </div>
            {{/if}}
          </div>

          {{#if (math count '>' 9)}}
          <div class="sb-pagination-container">
            <ul class="sb-pagination">
              {{#unless isFirstPage}}
                <li><a href="?p=1">«</a></li>
                <li><a href="?p={{math page '-' 1}}">‹</a></li>
              {{/unless}}
              
              {{#each (range (math (math page '-' 2) 'max' 1) (math (math page '+' 2) 'min' count))}}
                <li class="{{#compare this '==' ../page}}sb-active{{/compare}}">
                  <a href="?p={{this}}">{{this}}</a>
                </li>
              {{/each}}
              
              {{#unless isLastPage}}
                <li><a href="?p={{math page '+' 1}}">›</a></li>
                <li><a href="?p={{count}}">»</a></li>
              {{/unless}}
            </ul>
          </div>
          {{/if}}
        </div>
      </section>
      <!-- blog list end -->

