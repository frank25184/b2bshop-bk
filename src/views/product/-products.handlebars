<div class="sb-masonry-grid">
  <div class="sb-grid-sizer"></div>
  {{#each products}}
  <div class="sb-grid-item sb-item-33 {{this.categories}}">
    <a data-fancybox="menu" href="{{this.image}}" class="sb-menu-item sb-mb-30">
      <div class="sb-cover-frame">
        <img src="{{this.image}}" alt="{{this.title}}">
        {{#if this.badges}}
        <div class="sb-badge {{this.badgeClass}}">
          <i class="{{this.badgeIcon}}"></i> {{this.badgeText}}
        </div>
        {{/if}}
      </div>
      <div class="sb-card-tp">
        <h4 class="sb-card-title">{{this.title}}</h4>
        <div class="sb-price"><sub>$</sub> {{this.price}}</div>
      </div>
      <div class="sb-description">
        {{{this.description}}}
      </div>
    </a>
  </div>
  {{/each}}
</div>
        </div>
