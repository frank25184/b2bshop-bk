      {{#extend "css"}}
       <style>
/* ‰∏ªÂàÜÁ±ªÊ†∑Âºè */
.sb-main-categories {
  display: flex;
  justify-content: center;
  gap: 20px;
  margin-bottom: 25px;
}

.sb-main-categories .sb-filter-link {
  font-size: 18px;
  font-weight: 600;
  color: #666;
  padding: 0 14px;
  border-radius: 25px;
  transition: all 0.3s ease;
  background: transparent;
  border: 2px solid #eee;
}

.sb-main-categories .sb-filter-link:hover,
.sb-main-categories .sb-filter-link.sb-active {
  background: #9CCC65;
  color: #fff;
  border-color: #9CCC65;
}

/* Â≠êÂàÜÁ±ªÊ†∑Âºè */
.sb-sub-categories {
  display: flex;
  justify-content: center;
  gap: 15px;
  margin-top: 20px;
  padding-top: 20px;
  border-top: 1px solid rgba(0, 0, 0, 0.08);
}

.sb-sub-categories .sb-filter-link {
  font-size: 14px;
  color: #888;
  padding: 0 15px;
  border-radius: 15px;
  transition: all 0.3s ease;
  background: #f5f5f5;
}

.sb-sub-categories .sb-filter-link:hover,
.sb-sub-categories .sb-filter-link.sb-active {
  background: #666;
  color: #fff;
}

/* ÂìçÂ∫îÂºèË∞ÉÊï¥ */
@media (max-width: 768px) {
  .sb-main-categories,
  .sb-sub-categories {
    flex-wrap: wrap;
  }
  
  .sb-main-categories .sb-filter-link {
    font-size: 16px;
    padding: 8px 15px;
  }
  
  .sb-sub-categories .sb-filter-link {
    font-size: 13px;
    padding: 5px 12px;
  }
}

/***add category css**/
.category-icon {
  margin-right: 5px;
  font-size: 1.2em;
}

.sb-filter-link {
  display: flex;
  align-items: center;
  justify-content: center;
}

.sb-sub-categories {
  opacity: 0;
  height: 0;
  transition: all 0.3s ease;
}

.sb-sub-categories.active {
  opacity: 1;
  height: auto;
}
.sb-masonry-grid.sb-loading {
  opacity: 0.5;
  pointer-events: none;
}

.sb-error,
.sb-no-results {
  text-align: center;
  padding: 30px;
  color: #666;
  font-size: 16px;
}

.sb-error {
  color: #f44336;
}
.sb-product-meta {
  margin-top: 10px;
  display: flex;
  flex-wrap: wrap;
  gap: 10px;
}

.sb-meta-item {
  font-size: 12px;
  color: #666;
  display: flex;
  align-items: center;
  gap: 5px;
}

.sb-meta-item i {
  font-size: 14px;
  color: #9CCC65;
}

.sb-badge {
  position: absolute;
  top: 10px;
  right: 10px;
  padding: 5px 10px;
  border-radius: 15px;
  font-size: 12px;
  display: flex;
  align-items: center;
  gap: 5px;
}

.sb-badge.sb-certified {
  background: #4CAF50;
  color: white;
}

.sb-badge.sb-training {
  background: #2196F3;
  color: white;
}

       </style>
