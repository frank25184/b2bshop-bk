{{#extend "js"}}
    <script>
      document.addEventListener('DOMContentLoaded', (event) => {
          var loadMoreButton = document.getElementById('more');
          var currentPage = 1;
          var currentPagePath = window.location.pathname;
          loadMoreButton.addEventListener('click', function() {
          currentPage++;
          var xhr = new XMLHttpRequest();
          xhr.open('GET', currentPagePath +'?p=' + currentPage, true);
          xhr.onreadystatechange = function() {
            if (xhr.readyState === 4 && xhr.status === 200) {
                var data = JSON.parse(xhr.responseText);
                if (data.products && data.products.length > 0) {
                  var articlesList = document.getElementById('prolist');
                  data.products.forEach(function(product) {
                    var colDiv = document.createElement('div');
                    colDiv.className = 'col col-4 col-d-6 col-t-12';
                    var articleDiv = document.createElement('div');
                    articleDiv.className = 'article';
                    var articleInnerDiv = document.createElement('div');
                    articleInnerDiv.className = 'article__inner';
                    var articleContent = document.createElement('div');
                    articleContent.className = 'article__content';
                    var authorImageDiv = document.createElement('div');
                    authorImageDiv.className = 'post__author__image catThumb';
                    var img = document.createElement('img');
                    img.setAttribute('src', '/upload/sites/thumbnail/' + product.imgs[0]);
                    img.alt = product.name;
                    authorImageDiv.appendChild(img);
                    var articleTitle = document.createElement('h3');
                    articleTitle.className = 'article__title';
                    var titleLink = document.createElement('a');
                    titleLink.href = '/review/' + product.name_changed;
                    titleLink.textContent = product.name; 
                    articleTitle.appendChild(titleLink);
                    var articleInfo = document.createElement('div');
                    articleInfo.className = 'article__info';
                    var infoParagraph = document.createElement('p');
                    infoParagraph.style.marginBottom = '0';
                    infoParagraph.textContent = product.intro;
                    articleInfo.appendChild(infoParagraph);
                    articleContent.appendChild(authorImageDiv);
                    articleContent.appendChild(articleTitle);
                    articleContent.appendChild(articleInfo);
                    articleInnerDiv.appendChild(articleContent);
                    articleDiv.appendChild(articleInnerDiv);
                    colDiv.appendChild(articleDiv);
                    articlesList.appendChild(colDiv);
                  });
                  if (data.isLastPage) {
                   loadMoreButton.innerHTML = 'More Coming Soon';
                   loadMoreButton.style.backgroundColor = "#e3e3e3";
                   loadMoreButton.disabled = true;
                  }
                } else {
                  console.error('error, More Coming Soon');
                }
            }
          };
          xhr.onerror = function() {
            console.error('Error loading more projects');
            loadMoreButton.innerHTML = 'More Coming Soon!';
            loadMoreButton.disabled = true;
          };
          xhr.send();
          });
      });
    </script>
